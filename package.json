{
  "name": "andmemory",
  "version": "0.1.0",
  "description": "GUI for measuring memory usage of an Android app, deployed on a device, using the sysdump memory report",
  "main": "app.js",
  "scripts": {
    "test_mem": "node ./tests/measureMemory_test.js",
    "run_app": "electron ./app.js",
    "package": "electron-builder --dir",
    "distribute": "electron-builder"
  },
  "dependencies": {},
  "devDependencies": {
    "electron": "^8.1.1",
    "electron-builder": "^22.9.1"
  },
  "build": {
    "appId": "com.ar.andmemory",
    "productName": "Android Memory Helper",
    "copyright": "MIT license",
    "directories": {
      "output": "./build/${version}"
    }
  },
  "author": "Adrian Rutkiewicz",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/serpenta/node_android_memory.git"
  },
  "bugs": {
    "url": "https://github.com/serpenta/node_android_memory/issues"
  },
  "homepage": "https://github.com/serpenta/node_android_memory#readme"
}
